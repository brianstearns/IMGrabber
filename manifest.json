{
    "manifest_version": 3,
    "name": "IMGrabber",
    "version": "1.0",
    "version_name": "1.0.0",
    "homepage_url": "https://github.com/brianstearns/IMGrabber",
    "short_name": "IMGrabber",
    "description": "Download all images from any webpage",
    "permissions": [
        "activeTab",
        "downloads",
        "contextMenus"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "32": "icons/icon32.png",
            "48": "icons/icon48.png"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}